{
    "name": "vnn/wordpress-rest-api-client",
    "config": {
      "sort-packages": true
    },
    "require": {
      "php": ">=7.4",
      "guzzlehttp/guzzle": "^6.0||^7.0",
      "psr/http-message": ">=1.0"
    },
    "require-dev": {
      "php-parallel-lint/php-parallel-lint": "^1.3",
      "peridot-php/peridot": "^1.19",
      "peridot-php/leo": "^1.6",
      "squizlabs/php_codesniffer": "^3.7",
      "peridot-php/peridot-prophecy-plugin": "^1.1"
    },
    "autoload": {
      "psr-4": {
        "Vnn\\WpApiClient\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Vnn\\WpApiClient\\tests\\": "test/"
      }
    },
    "scripts": {
      "phpcs": "vendor/bin/phpcs -sp --colors",
      "phpcbf": "vendor/bin/phpcbf -sp",
      "parallel-lint": "vendor/bin/parallel-lint src specs peridot.php",
      "peridot": "vendor/bin/peridot"
    }
}
